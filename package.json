{
  "name": "arikushi",
  "version": "1.0.0",
  "description": "Generate Japanese word mapping from Latin characters",
  "main": "./lib/Converter.js",
  "types": "./lib/Converter.d.ts",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "prepublish": "tsc",
    "report-coverage": "nyc report --reporter=text-lcov | codecov",
    "start": "ts-node src/Converter.ts",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/serkansokmen/arikushi.git"
  },
  "keywords": [
    "generate",
    "latin",
    "japanese",
    "string",
    "mapping"
  ],
  "author": "Serkan Sokmen <e.serkan.sokmen@gmail.com> (http://serkansokmen.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serkansokmen/arikushi/issues"
  },
  "homepage": "https://github.com/serkansokmen/arikushi#readme",
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^29.5.2",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.0",
    "chai-string": "^1.5.0",
    "codecov": "^3.8.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "ghooks": "^2.0.4",
    "jest": "^29.5.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "semantic-release": "^18.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
